{
  "templates": [
    {
      "id": "default",
      "name": "Styled H1",
      "content": "<h1 style=\" text-align: center; font-size: 3em; font-family: 'Georgia', serif; color: #2c3e50; margin: 0.5em 0; padding: 10px 0; border-top: 4px solid #3498db; border-bottom: 4px solid #3498db; text-transform: uppercase; letter-spacing: 3px;\">{{selectedText}}</h1>",
      "params": [
        "selectedText"
      ],
      "defaultValues": {
        "selectedText": "Welcome to Mustache Template."
      }
    },
    {
      "id": "1746685230032",
      "name": "YellowBgText",
      "content": "<font style=\"background-color: #FFFACD; color:{{color}}; padding: 4px; border-radius: 8px; text-shadow: 1px 1px 2px rgba(0,0,0,0.1); font-weight: bold;\">{{selectedText}}</font>",
      "params": [
        "color",
        "selectedText"
      ],
      "defaultValues": {
        "selectedText": "",
        "color": "black"
      },
      "isCommand": true
    },
    {
      "id": "1746685411514",
      "name": "RedBgText",
      "content": "<font style=\"background-color:tomato; color:white; padding:4px; text-shadow: 1px 1px 2px rgba(0,0,0,0.2);font-weight: bold;\">{{selectedText}}</font>",
      "params": [
        "selectedText"
      ],
      "defaultValues": {
        "selectedText": ""
      },
      "isCommand": true
    },
    {
      "id": "1746849248934",
      "name": "CouseraURL",
      "content": "<a href=\"{{selectedText}}\" target=\"_parent\" style=\"display: flex; align-items: center; height: 100%;\"><img src=\"https://raw.githubusercontent.com/jimmy-pink/colab-playground/refs/heads/main/resources/icon/coursera.svg\" alt=\"Open In Coursera\" style=\"height: 100%; object-fit: contain;\"><span style=\"height: 100%; display: flex; align-items: center; padding-left: 10px;\">{{courseName}}</span></a>",
      "params": [
        "selectedText",
        "courseName"
      ],
      "defaultValues": {
        "courseName": "",
        "selectedText": ""
      },
      "isCommand": false
    },
    {
      "id": "1747649108295",
      "name": "快速上手",
      "content": "<font style=\"background:#F6E1AC;color:#664900\">🌰 快速上手 </font>[ ✍️ {{title}}]({{link}})",
      "params": [
        "title",
        "link"
      ],
      "defaultValues": {
        "link": "",
        "title": ""
      },
      "isCommand": false
    }
  ]
}